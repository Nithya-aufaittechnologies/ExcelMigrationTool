{
  "openapi": "3.0.1",
  "info": {
    "title": "Excel Migration API",
    "description": "API for migrating Excel data to SQL Server tables",
    "version": "v1"
  },
  "paths": {
    "/api/excel/upload": {
      "post": {
        "tags": [
          "ExcelMigration"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": [
                  "targetSchema",
                  "targetTable",
                  "userId"
                ],
                "type": "object",
                "properties": {
                  "excelFile": {
                    "type": "string",
                    "format": "binary"
                  },
                  "targetSchema": {
                    "type": "string"
                  },
                  "targetTable": {
                    "type": "string"
                  },
                  "userId": {
                    "type": "integer",
                    "format": "int64"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UploadResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UploadResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "RowErrorDetail": {
        "type": "object",
        "properties": {
          "rowNumber": {
            "type": "integer",
            "format": "int32"
          },
          "columnName": {
            "type": "string",
            "nullable": true
          },
          "value": {
            "nullable": true
          },
          "errorMessage": {
            "type": "string",
            "nullable": true
          },
          "rowData": {
            "type": "object",
            "additionalProperties": {
              "nullable": true
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "UploadResponse": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean"
          },
          "rowsInserted": {
            "type": "integer",
            "format": "int32"
          },
          "rowsUpdated": {
            "type": "integer",
            "format": "int32"
          },
          "rowsFailed": {
            "type": "integer",
            "format": "int32"
          },
          "errorMessages": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          },
          "rowErrors": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/RowErrorDetail"
            },
            "nullable": true
          },
          "message": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}